{
  "name": "rsbuild-react-base",
  "version": "0.1.0",
  "author": "agustinusnathaniel",
  "private": true,
  "type": "module",
  "engines": {
    "node": ">=24.11.x"
  },
  "packageManager": "pnpm@10.27.0",
  "pnpm": {
    "neverBuiltDependencies": []
  },
  "scripts": {
    "dev": "rsbuild dev",
    "build": "rsbuild build",
    "build-and-analyze": "cross-env RSDOCTOR=true pnpm build",
    "preview": "rsbuild preview",
    "biome:check": "biome check --no-errors-on-unmatched --error-on-warnings",
    "biome:fix": "pnpm biome:check --write",
    "biome:ci": "biome ci",
    "type:check": "tsc --noEmit",
    "test": "rstest",
    "turbo": "pnpm dlx turbo@2.7.2",
    "check:turbo": "pnpm turbo biome:check type:check test",
    "up-interactive": "pnpm up -i",
    "up-latest": "pnpm up-interactive -L",
    "release": "cross-env HUSKY=0 standard-version",
    "push-release": "git push --follow-tags origin main",
    "prepare": "husky",
    "knip": "knip"
  },
  "dependencies": {
    "@fontsource-variable/plus-jakarta-sans": "^5.2.8",
    "@tanstack/react-router": "^1.144.0",
    "@tanstack/react-router-devtools": "^1.144.0",
    "react": "^19.2.3",
    "react-dom": "^19.2.3",
    "react-icons": "^5.5.0"
  },
  "devDependencies": {
    "@biomejs/biome": "2.3.10",
    "@commitlint/cli": "^20.3.0",
    "@commitlint/config-conventional": "^20.3.0",
    "@commitlint/cz-commitlint": "^20.3.0",
    "@commitlint/types": "^20.2.0",
    "@rsbuild/core": "1.7.1",
    "@rsbuild/plugin-assets-retry": "1.5.0",
    "@rsbuild/plugin-babel": "^1.0.6",
    "@rsbuild/plugin-image-compress": "^1.3.1",
    "@rsbuild/plugin-react": "1.4.2",
    "@rsbuild/plugin-svgr": "1.2.3",
    "@rsbuild/plugin-type-check": "1.3.2",
    "@rsdoctor/rspack-plugin": "^1.4.0",
    "@rstest/core": "^0.7.8",
    "@tanstack/router-plugin": "^1.145.2",
    "@types/node": "^25.0.3",
    "@types/react": "^19.2.7",
    "@types/react-dom": "^19.2.3",
    "babel-plugin-react-compiler": "^1.0.0",
    "commit-and-tag-version": "^12.6.1",
    "commitizen": "^4.3.1",
    "cross-env": "^10.1.0",
    "husky": "9.1.7",
    "jsdom": "^27.4.0",
    "knip": "^5.79.0",
    "lint-staged": "^16.2.7",
    "typescript": "^5.9.3"
  }
}
