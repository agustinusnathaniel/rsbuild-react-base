{
  "name": "rsbuild-react-base",
  "version": "0.1.0",
  "author": "sozonome",
  "private": true,
  "dependencies": {
    "@fontsource/plus-jakarta-sans": "^5.0.22",
    "react": "^18.3.1",
    "react-dom": "^18.3.1",
    "react-helmet": "^6.1.0",
    "react-icons": "^5.3.0",
    "react-router-dom": "^6.26.2",
    "web-vitals": "^4.2.3"
  },
  "scripts": {
    "dev": "rsbuild dev",
    "build": "rsbuild build",
    "build-and-analyze": "cross-env RSDOCTOR=true pnpm build",
    "preview": "rsbuild preview",
    "lint": "pnpm biome lint ./src",
    "toolchain:fix": "pnpm biome check --apply ./src rsbuild.config.ts commitlint.config.js",
    "type-check": "tsc --noEmit",
    "turbo": "pnpm dlx turbo@2.0.9",
    "check:turbo": "pnpm turbo lint type-check",
    "up-interactive": "pnpm up -i",
    "up-latest": "pnpm up-interactive -L",
    "release": "cross-env HUSKY=0 standard-version",
    "push-release": "git push --follow-tags origin main",
    "prepare": "husky"
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "devDependencies": {
    "@biomejs/biome": "1.8.3",
    "@commitlint/cli": "^19.4.1",
    "@commitlint/config-conventional": "^19.4.1",
    "@commitlint/cz-commitlint": "^19.4.0",
    "@rsbuild/core": "1.0.1",
    "@rsbuild/plugin-assets-retry": "1.0.1",
    "@rsbuild/plugin-image-compress": "^1.0.2",
    "@rsbuild/plugin-react": "1.0.1",
    "@rsbuild/plugin-svgr": "1.0.1",
    "@rsbuild/plugin-type-check": "1.0.1",
    "@rsdoctor/rspack-plugin": "^0.4.1",
    "@testing-library/jest-dom": "^6.5.0",
    "@testing-library/react": "^16.0.1",
    "@testing-library/user-event": "^14.5.2",
    "@types/jest": "^29.5.12",
    "@types/node": "^22.5.4",
    "@types/react": "^18.3.5",
    "@types/react-dom": "^18.3.0",
    "@types/react-helmet": "^6.1.11",
    "commitizen": "^4.3.0",
    "cross-env": "^7.0.3",
    "husky": "9.1.5",
    "lint-staged": "^15.2.10",
    "standard-version": "^9.5.0",
    "typescript": "^5.6.2"
  },
  "engines": {
    "node": ">=20.14.x",
    "pnpm": ">=9"
  },
  "packageManager": "pnpm@9.1.1"
}
