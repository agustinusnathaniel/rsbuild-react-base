{
  "name": "rsbuild-react-base",
  "version": "0.1.0",
  "author": "agustinusnathaniel",
  "private": true,
  "engines": {
    "node": ">=22.11.x",
    "pnpm": ">=9"
  },
  "packageManager": "pnpm@9.13.2",
  "dependencies": {
    "@fontsource-variable/plus-jakarta-sans": "^5.1.1",
    "react": "^19.0.0",
    "react-dom": "^19.0.0",
    "react-helmet-async": "^2.0.5",
    "react-icons": "^5.4.0",
    "react-router": "^7.1.1"
  },
  "scripts": {
    "dev": "rsbuild dev",
    "build": "rsbuild build",
    "build-and-analyze": "cross-env RSDOCTOR=true pnpm build",
    "preview": "rsbuild preview",
    "biome:check": "biome check",
    "biome:fix": "pnpm biome:check --write",
    "biome:ci": "biome ci",
    "type:check": "tsc --noEmit",
    "turbo": "pnpm dlx turbo@2.0.9",
    "check:turbo": "pnpm turbo biome:check type:check",
    "up-interactive": "pnpm up -i",
    "up-latest": "pnpm up-interactive -L",
    "release": "cross-env HUSKY=0 standard-version",
    "push-release": "git push --follow-tags origin main",
    "prepare": "husky",
    "knip": "knip"
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "devDependencies": {
    "@biomejs/biome": "1.9.4",
    "@commitlint/cli": "^19.6.1",
    "@commitlint/config-conventional": "^19.6.0",
    "@commitlint/cz-commitlint": "^19.6.1",
    "@commitlint/types": "^19.5.0",
    "@rsbuild/core": "1.1.13",
    "@rsbuild/plugin-assets-retry": "1.0.7",
    "@rsbuild/plugin-image-compress": "^1.1.0",
    "@rsbuild/plugin-react": "1.1.0",
    "@rsbuild/plugin-svgr": "1.0.6",
    "@rsbuild/plugin-type-check": "1.2.0",
    "@rsdoctor/rspack-plugin": "^0.4.12",
    "@types/node": "^22.10.3",
    "@types/react": "^19.0.2",
    "@types/react-dom": "^19.0.2",
    "commitizen": "^4.3.1",
    "cross-env": "^7.0.3",
    "husky": "9.1.7",
    "knip": "^5.41.1",
    "lint-staged": "^15.3.0",
    "standard-version": "^9.5.0",
    "typescript": "^5.7.2"
  }
}
